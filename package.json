{
    "version": "1.0.0",
    "name": "sveltekit-projecttemplate-business",
    "description": "project template for sveltekit",
    "private": true,
    "author": "Jonas Froeller",
    "homepage": "https://sveltekit-projecttemplate-business.vercel.app",
    "repository": {
        "type": "git",
        "url": "https://github.com/jonasfroeller/sveltekit-projecttemplate-business"
    },
    "keywords": [
        "template",
        "svelte",
        "svelte-kit"
    ],
    "license": "MIT",
    "funding": "https://buymeacoffee.com/jonasfroeller",
    "scripts": {
        "all": "npm-run-all --parallel dev check",
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test:unit": "vitest",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "commit": "cz",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@iconify/svelte": "^3.1.4",
        "@playwright/test": "^1.28.1",
        "@sveltejs/adapter-auto": "^2.1.0",
        "@sveltejs/adapter-cloudflare": "^2.3.0",
        "@sveltejs/adapter-netlify": "^2.0.7",
        "@sveltejs/adapter-node": "^1.3.0",
        "@sveltejs/adapter-vercel": "^3.0.3",
        "@sveltejs/kit": "^1.20.4",
        "@tailwindcss/typography": "^0.5.9",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "autoprefixer": "^10.4.15",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-svelte": "^2.30.0",
        "husky": "^8.0.0",
        "npm-run-all": "^1.8.0",
        "postcss": "^8.4.28",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "prisma": "^5.3.1",
        "svelte": "^4.0.0",
        "svelte-check": "^3.4.3",
        "sveltekit-superforms": "^1.1.2",
        "tailwindcss": "^3.3.3",
        "tslib": "^2.6.0",
        "typescript": "^5.0.0",
        "vite": "^4.3.6",
        "vitest": "^0.25.8",
        "zod": "^3.21.4"
    },
    "type": "module",
    "dependencies": {
        "@lucia-auth/adapter-prisma": "^3.0.1",
        "@prisma/client": "^5.3.1",
        "@types/ioredis": "5.0.0",
        "clsx": "^2.0.0",
        "ioredis": "5.3.2",
        "lucia": "^2.6.0",
        "lucide-svelte": "^0.268.0",
        "prisma-redis-middleware": "4.8.0",
        "tailwind-merge": "^1.14.0",
        "tailwind-variants": "^0.1.13",
        "tailwindcss-animate": "^1.0.6"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    }
}