{
	"version": "1.0.0",
	"name": "sveltekit-projecttemplate-business",
	"description": "project template for sveltekit",
	"private": true,
	"author": "Jonas Froeller",
	"homepage": "https://sveltekit-projecttemplate-business.vercel.app",
	"repository": {
		"type": "git",
		"url": "https://github.com/jonasfroeller/sveltekit-projecttemplate-business"
	},
	"keywords": [
		"template",
		"svelte",
		"svelte-kit"
	],
	"license": "MIT",
	"funding": "https://buymeacoffee.com/jonasfroeller",
	"scripts": {
		"all": "npm-run-all --parallel dev check",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"commit": "cz",
		"prepare": "husky install && npx prisma generate",
		"generate-favicons": "npm-run-all generate-dark-favicons generate-light-favicons",
		"generate-dark-favicons": "pwa-asset-generator ./static/favicons/favicon-dark.svg ./static/favicons/darkmode --index ./src/app.html --path-override '%sveltekit.assets%/favicons/darkmode' --type png --opaque false --dark-mode --favicon --n",
		"generate-light-favicons": "pwa-asset-generator ./static/favicons/favicon-light.svg ./static/favicons/lightmode --index ./src/app.html --path-override '%sveltekit.assets%/favicons/lightmode' --type png --opaque false --favicon --n"
	},
	"devDependencies": {
		"@iconify/svelte": "^3.1.4",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/typography": "^0.5.10",
		"@types/html-minifier": "^4.0.4",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.15",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.30.0",
		"husky": "^8.0.0",
		"npm-run-all": "^1.8.0",
		"postcss": "^8.4.28",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"pwa-asset-generator": "^6.3.1",
		"rtlcss": "^4.1.1",
		"sass": "^1.69.5",
		"svelte": "^4.2.2",
		"svelte-check": "^3.4.3",
		"sveltekit-superforms": "^1.1.2",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.0",
		"vitest": "^1.0.0",
		"zod": "^3.21.4"
	},
	"type": "module",
	"dependencies": {
		"@builder.io/sdk-svelte": "^0.6.4",
		"@lucia-auth/adapter-prisma": "^3.0.1",
		"@lucia-auth/oauth": "^3.3.1",
		"@prisma/client": "^5.5.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"bits-ui": "^0.5.7",
		"clsx": "^2.0.0",
		"html-minifier": "^4.0.0",
		"ioredis": "5.3.2",
		"lucia": "^2.7.2",
		"lucide-svelte": "^0.268.0",
		"prisma-redis-middleware": "4.8.0",
		"svelte-persisted-store": "^0.7.0",
		"tailwind-merge": "^1.14.0",
		"tailwind-variants": "^0.1.16",
		"tailwindcss-animate": "^1.0.6"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	}
}
